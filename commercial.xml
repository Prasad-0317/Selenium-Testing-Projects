<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="Suite">
  <test thread-count="5" name="ChromeTest">
    <parameter name="browser" value="chrome"/>
    <classes>
    	<class name="com.test.CommercialTest">
    		<methods>
    			<include name="testSearchProperty"/>
    		</methods>
    	</class>
    </classes>
  </test> <!-- Test -->
  <test thread-count="5" name="EdgeTest">
    <parameter name="browser" value="edge"/>
    <classes>
    	<class name="com.test.CommercialTest">
    		<methods>
    			<include name="testSearchProperty"/>
    		</methods>
    	</class>
    </classes> 
  </test>
  
  <test thread-count="5" name="Others_EdgeTest">
    <classes>
    	<class name="com.test.CommercialTest">
    		<methods>
    			<exclude name="testSearchProperty"/>
    		</methods>
    	</class>
    </classes> 
  </test>
</suite> <!-- Suite -->
